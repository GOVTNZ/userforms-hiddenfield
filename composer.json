{
    "name": "govtnz/silverstripe-userforms-hiddenfield",
    "description": "Adds an HiddenField to silverstripe/userforms",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "userforms", "HiddenField"],
    "authors": [{
        "name": "Juan van den Anker",
        "email": "juan@vdanker.net"
    }],
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/userforms": "*"
    },
    "scripts": {
        "remove-uses": "php-cs-fixer fix src/ --rules=no_unused_imports",
        "lint": "phpcs --extensions=php src/",
        "syntax-check": "find src/",
        "lint-clean": "phpcbf src/"
    },
    "autoload": {
        "psr-4": {
            "GovtNZ\\SilverStripe\\UserForms\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
